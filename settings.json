{
    "colorscheme": "gruvbox",
    "diff": false,
    "literate": false,
    "lsp.formatOnSave": true,
    "lsp.server": "lua=lua-lsp, rust=rls, python=pyls, go=gopls, javascript=deno, typescript=deno lsp={\"importMap\": \"./import_map.json\"}",
    "savecursor": true,
    "saveundo": true,
    "softwrap": true,
    "statusformatl": " $(modified)$(filename) | $(status.size) ",
    "statusformatr": "  $(opt:encoding) | $(opt:fileformat) | $(opt:filetype) | $(line):$(col) "
}
